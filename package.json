{
   "name": "tp-node",
   "version": "1.0.0",
   "description": "",
   "main": "dist/app.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc",
      "watch": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"npm run tsc-watch\" \"npm run node-watch\"",
      "tsc-watch": "tsc --watch",
      "node-watch": "nodemon dist/app.js",
      "eslint": "eslint --ext .ts ./src ./test",
      "eslint:fix": "eslint  --ext .ts --fix ./src ./test",
      "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register test/**/*.ts --exit",
      "generate-migration": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
      "migration": "ts-node ./node_modules/.bin/typeorm migration:run",
      "revert-migration": "ts-node ./node_modules/.bin/typeorm migration:revert",
      "postinstall": "npm run migration"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/bersiroth/tp-node.git"
   },
   "author": "Bernard CARON",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/bersiroth/tp-node/issues"
   },
   "homepage": "https://github.com/bersiroth/tp-node#readme",
   "dependencies": {
      "config": "^3.1.0",
      "express": "^4.17.0",
      "module-alias": "^2.2.0",
      "pg": "^7.11.0",
      "pg-hstore": "^2.3.3",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.18"
   },
   "devDependencies": {
      "@hapi/joi": "^15.0.3",
      "@types/chai": "^4.1.7",
      "@types/chai-http": "^4.2.0",
      "@types/config": "0.0.34",
      "@types/express": "^4.17.0",
      "@types/hapi__joi": "^15.0.1",
      "@types/mocha": "^5.2.7",
      "@types/node": "^8.0.29",
      "@types/validator": "^10.11.1",
      "@typescript-eslint/eslint-plugin": "^1.10.2",
      "@typescript-eslint/parser": "^1.10.2",
      "chai": "^4.2.0",
      "chai-http": "^4.3.0",
      "concurrently": "^4.1.0",
      "eslint": "^5.16.0",
      "eslint-plugin-chai-friendly": "^0.4.1",
      "mocha": "^6.1.4",
      "nodemon": "^1.19.1",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.8.0",
      "typescript": "3.3.3333"
   },
   "_moduleAliases": {
      "tp-node": "dist/"
   }
}
